{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "target": "es6",
      "sourceMap": true,
      "outDir": "./.build",
      "baseUrl": "./"
    },
    "exclude": ["node_modules"]
}
  